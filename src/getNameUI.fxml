<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="213.0" prefWidth="340.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <TextField fx:id="nameField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Your Name Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <TextField fx:id="ipField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Server IP Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <TextField fx:id="portField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Server PORT Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <Button fx:id="goButton" text="GO!">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</VBox>
