<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="286.0" prefWidth="399.0" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <TextField fx:id="nameField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Your Name Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <TextField fx:id="ipField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Server IP Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
    <TextField fx:id="portField" alignment="CENTER" maxWidth="200" minWidth="200" promptText="Input Server PORT Here">
        <font>
            <Font size="17.0" />
        </font>
    </TextField>
   <HBox alignment="CENTER" prefHeight="42.0" prefWidth="399.0" spacing="20.0">
      <children>
         <Label text="Select Background">
            <font>
               <Font size="16.0" />
            </font>
         </Label>
         <ComboBox id="backgroundSelector" fx:id="backgroundSelector" prefWidth="150.0" promptText="Select Background" />
      </children>
   </HBox>
    <Button fx:id="goButton" text="GO!">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</VBox>
